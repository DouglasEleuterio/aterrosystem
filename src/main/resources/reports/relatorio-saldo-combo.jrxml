	<queryString>
		<![CDATA[select trs.trs_ds_nome,
       cmb.cmb_vl_saldo,
       aqs.aqs_vl_pago / aqs.aqs_qt_adquirida * cmb_vl_saldo as saldo_financeiro,
       ttd.tds_ds_nome,
       aqs.aqs_dt_pagamento,
       aqs.aqs_qt_adquirida,
       aqs.aqs_vl_pago
from cmb_combo cmb
         inner join trs_transportador trs on cmb.trs_id = trs.trs_id
         inner join tds_tipo_descarte ttd on cmb.tds_id = ttd.tds_id
         inner join aqs_aquisicao aqs on cmb.cmb_id = aqs.cmb_id

WHERE cmb.cmb_st_ativo = true
  AND cmb.cmb_vl_saldo > 0
order by trs_ds_nome;]]>
	</queryString>
